        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ItemsControl>
                        <!--ExistingRowAddUndoAndSave actually saves any changes made in UnifiedCellValueChanging within CellValueChanging, so CellValueChanging must be added first-->
                        <dxmvvm:EventToCommand Command="{Binding DataContext.CellValueChangingCommand, ElementName=userControl}" EventName="CellValueChanging" PassEventArgsToCommand="True" />
